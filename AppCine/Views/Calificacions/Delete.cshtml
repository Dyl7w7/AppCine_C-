@model AppCine.Models.Calificacion

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar calificación</h1>

<h3>¿Está seguro que desea eliminar esta calificación?</h3>
<div>
    <h4>Calificación</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Calificacion1)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Calificacion1)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdpeliculaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdpeliculaNavigation.Idpelicula)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Idcalificacion" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
    </form>
</div>
